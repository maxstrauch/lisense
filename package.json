{
  "name": "lisense",
  "version": "1.6.1",
  "description": "A simple but working CLI tool to extract NPM package licenses reliably",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --collectCoverageFrom \"(index|lisense).js\" --coverageDirectory \"./coverage/\" --coverageReporters lcov",
    "test:parsing": "jasmine --config=jasmine.json --reporter=jasmine-console-reporter --no-color || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxstrauch/lisense.git"
  },
  "keywords": [
    "npm",
    "license",
    "extractor",
    "tool",
    "cli"
  ],
  "bin": {
    "lisense": "./index.js"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "maxstrauch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxstrauch/lisense/issues"
  },
  "homepage": "https://github.com/maxstrauch/lisense#readme",
  "dependencies": {
    "ascii-table": "0.0.9",
    "chalk": "^4.0.0",
    "commander": "^4.1.1",
    "debug": "^4.3.4",
    "jasmine": "^4.6.0",
    "jasmine-console-reporter": "^3.1.0"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "mock-fs": "^5.2.0"
  }
}
